<?xml version="1.0"?>
<configuration>
  <configSections>
    <sectionGroup name="DataGenericBaseGroup">
      <section name="EdmDataGenericBaseSection" 
               type="Nequeo.Data.Configuration.EdmDataGenericBaseSection, Nequeo.Data.LinqTo.Edm, Version=2014.26.1.1, Culture=neutral, PublicKeyToken=731a1445b7d27437" 
               allowLocation="true" 
               allowDefinition="Everywhere"/>
      <section name="EdmDataGenericBaseDefaultSection" 
               type="Nequeo.Data.Configuration.EdmDataGenericBaseDefaultSection, Nequeo.Data.LinqTo.Edm, Version=2014.26.1.1, Culture=neutral, PublicKeyToken=731a1445b7d27437" 
               allowLocation="true" 
               allowDefinition="Everywhere"/>
    </sectionGroup>
  </configSections>

  <!-- cacheTimeOut is in seconds -->
  <DataGenericBaseGroup>
    <EdmDataGenericBaseDefaultSection edmDataName="edmDataName">
      <EdmData cacheTimeOut="120"/>
    </EdmDataGenericBaseDefaultSection>
    <EdmDataGenericBaseSection>
      <EdmData>
        <add name="uniqueName" 
             databaseName="databaseName" 
             tableName="tableName" 
             referenceLazyLoading="false"/>
      </EdmData>
    </EdmDataGenericBaseSection>
  </DataGenericBaseGroup>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
