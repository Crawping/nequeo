<?xml version="1.0"?>
<configuration>
    <configSections>
        <sectionGroup name="DataGenericBaseGroup">
            <section name="LinqToSqlDataGenericBaseSection"
                     type="Nequeo.Data.Configuration.LinqToSqlDataGenericBaseSection, Nequeo.Data.LinqTo.SqlServer, Version=2014.26.1.1, Culture=neutral, PublicKeyToken=731a1445b7d27437" 
                     allowLocation="true" 
                     allowDefinition="Everywhere"/>
            <section name="LinqToSqlDataGenericBaseDefaultSection" 
                     type="Nequeo.Data.Configuration.LinqToSqlDataGenericBaseDefaultSection, Nequeo.Data.LinqTo.SqlServer, Version=2014.26.1.1, Culture=neutral, PublicKeyToken=731a1445b7d27437" 
                     allowLocation="true" 
                     allowDefinition="Everywhere"/>
        </sectionGroup>
    </configSections>

    <!-- cacheTimeOut is in seconds -->
    <DataGenericBaseGroup>
        <LinqToSqlDataGenericBaseDefaultSection linqDataName="linqDataName">
            <LinqData cacheTimeOut="120"/>
        </LinqToSqlDataGenericBaseDefaultSection>
        <LinqToSqlDataGenericBaseSection>
            <LinqData>
                <add name="uniqueName" 
                     databaseName="databaseName" 
                     tableName="tableName" 
                     referenceLazyLoading="false"/>
            </LinqData>
        </LinqToSqlDataGenericBaseSection>
    </DataGenericBaseGroup>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>
</configuration>
